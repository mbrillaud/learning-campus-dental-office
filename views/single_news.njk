{% extends "./layout.njk" %}

{% block content %}
    <h1 class="text-center pt-5 pb-5">{{ news.title }}</h1>
    <div class="single-news">
        <div class="img-container">
            <img src="../{{ news.imageUrl}}" alt="{{ news.title }}">
        </div>
        {{ news.content | safe}}
    </div>

{% endblock %}